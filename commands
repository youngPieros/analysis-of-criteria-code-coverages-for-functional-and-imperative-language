# PHASE 1

# search algorithms
bash ccs.sh linearSearching -dc
bash ccs.sh binarySearch -dc

# sort algorithms
bash ccs.sh bubbleSort -dc -bt
bash ccs.sh insertionSort -dc -bt
bash ccs.sh mergeSort -dc -bt
bash ccs.sh quickSort -dc -bt
bash ccs.sh selectionSort -dc -bt

# complex sorting
bash ccs.sh networkSorting -dc




# PHASE 2

# simple version of harder problems
bash ccs.sh nonDivisibleSubset -dc
bash ccs.sh queenAttack -dc -bt
bash ccs.sh scheduling -dc
bash ccs.sh webOfLies -dc

# harder problems
bash bccs.sh BatchWebOfLies -bt
bash bccs.sh BatchScheduling
bash bccs.sh BatchQueenAttack -bt
bash bccs.sh BatchNonDivisibleSubset -bt




